<!DOCTYPE html>
<html>
  <head>
    <title>Ebay</title>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
    <link href="assets/memenu.css" media="all" rel="stylesheet" type="text/css">
    <script src="assets/memenu.js" type="text/javascript"></script>
    <script>
      $(document).ready(function(){$(".memenu").memenu();});
    </script>
  </head>
  <body>
    <%= render 'layouts/header' %>
    <div class="text-danger text-center"> 
      <strong>
        <%= notice %>
        <%= alert %>
      </strong>
    </div>
    <%= yield %>
    <%= render 'layouts/footer' %>
    </div>
  </body>
</html>
