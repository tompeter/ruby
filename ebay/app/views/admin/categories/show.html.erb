<div class="row"> 
  <div class="col-sm-offset-3 col-md-6">
    <div class="panel panel-default">
      <div class="panel-heading">
        <%= link_to 'Delete', admin_categories_path, class: 'btn btn-default btn-xs pull-right', method: :delete %>
        <%= link_to 'Edit', edit_admin_category_path, class: 'btn btn-default btn-xs pull-right' %>
        <div class="panel-title">
          Category
        </div>
      </div>
      <div class="panel-body">
        <table  class="table table-bordered">  
          <thead></thead>
          <tbody>
            <tr>
              <td><p><%= @category.name %></p></td>
            </tr>
          </tbody>
        </table>
        <div class="col-md-offset-6">
          <%= link_to 'Back', admin_categories_path, class: 'btn btn-default'%>
        </div>
      </div>
    </div>
  </div>
</div>
         