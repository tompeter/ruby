<!DOCTYPE html>
<html>
<head>
  <title>Boot</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link href="assets/memenu.css" media="all" rel="stylesheet" type="text/css">
  <script src="assets/memenu.js" type="text/javascript"></script>
  <script>
$(document).ready(function(){$(".memenu").memenu();});
  </script>
  <script src="assets/simpleCart.min.js"></script>
</head>
<body>
	<div class="header">
    <div class="header-top">
      <div class="container">
        <div class="search">
          <form>
            <input onblur="if (this.value == '') {this.value = 'Search';}"
            onfocus="this.value = '';" type="text" value="Search ">
            <input type="submit" value="Go">
          </form>
        </div>

        <div class="header-left">
          <ul>
            <li>
              <a href="login.html">Login</a>
            </li>

            <li>
              <a href="register.html">Register</a>
            </li>
          </ul>

          <div class="cart box_1">
            <a href="checkout.html">
            <h3></h3>

            <div class="total">
              <h3><span class="simpleCart_total"></span> (<span class=
              "simpleCart_quantity" id="simpleCart_quantity"></span> items)
              <img alt="" src="assets/cart.png"></h3>
            </div></a>

            <p><a class="simpleCart_empty" href="javascript:;">Empty
            Cart</a></p>
          </div>

          <div class="clearfix"></div>
        </div>

        <div class="clearfix"></div>
      </div>
    </div>
<%= yield %>

<div class="footer">
